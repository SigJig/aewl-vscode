{
    "name": "aewl-language",
    "displayName": "AEWL Language",
    "description": "Syntax highlighting for AEWL",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "aewl",
            "aliases": ["AEWL", "aewl"],
            "extensions": [".aewl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "aewl",
            "scopeName": "source.aewl",
            "path": "./syntaxes/aewl.tmLanguage.json"
        }]
    }
}